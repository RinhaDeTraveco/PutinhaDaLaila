local Toggle8 = Section2:CreateToggle("UI Toggle", nil, function(State)
    Window:Toggle(State)
end)
 
Toggle8:CreateKeybind(tostring(Config.Keybind):gsub("Enum.KeyCode.", ""), function(Key)
    Config.Keybind = Enum.KeyCode[Key]
end)    
