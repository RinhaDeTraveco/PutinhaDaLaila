local hum = player.Character.HumanoidRootPart
local mouse = player:GetMouse() 
local tpservice= game:GetService("TeleportService")
local VirtualUser= game:service'VirtualUser'

local bLocation

-- Credits To Charwar for Server Hop
local PlaceID = game.PlaceId
local AllIDs = {}
local foundAnything = ""
local actualHour = os.date("!*t").hour
local Deleted = false
local File = pcall(function()
    AllIDs = game:GetService('HttpService'):JSONDecode(readfile("NotSameServers.json"))
end)
if not File then
    table.insert(AllIDs, actualHour)
    writefile("NotSameServers.json", game:GetService('HttpService'):JSONEncode(AllIDs))
end
 
 
function teleport(loc)
    bLocation = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    if game.Players.LocalPlayer.Character.Humanoid.Sit then
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
    end
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = loc
end

local JESUS = game.Players.LocalPlayer.Name
plr = game.Players.LocalPlayer


local Toggle2 = Section1:CreateToggle("Ignore the trash (not talking about me)", nil, function(State)
toggle = State 
        while toggle do
            wait(0)

instance = game.Workspace.Minerals:WaitForChild("MinedRock")

if game.Workspace.Minerals.MinedRock.Type.Value == "Iron" then print("Iron") instance.Name = "Iron"
	elseif game.Workspace.Minerals.MinedRock.Type.Value == "Bronze" then print("Bronze") instance.Name = "Bronze" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Copper" then print("Copper") instance.Name = "Copper" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Aluminium" then print("Aluminium") instance.Name = "Aluminium" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Silver" then print("Silver") instance.Name = "Silver" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Titanium" then print("Titanium") instance.Name = "Titanium" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Ruby" then print("Ruby") instance.Name = "Ruby" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Gold" then print("Gold") instance.Name = "Gold" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Quartz" then print("Quartz") instance.Name = "Quartz" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Cobalt" then print("Cobalt") instance.Name = "Cobalt" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Lithium" then print("Lithium") instance.Name = "Lithium" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Diamond" then print("Diamond") instance.Name = "Diamond" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Sapphire" then print("Sapphire") instance.Name = "Sapphire" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Jade" then print("Jade") instance.Name = "Jade" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Mercury" then print("Mercury") instance.Name = "Mercury" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Emerald" then print("Emerald") instance.Name = "Emerald" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Twitchium" then print("Twitchium") instance.Name = "Twitchium" elseif game.Workspace.Minerals.MinedRock.Type.Value == "Uranium" then print("Uranium") instance.Name = "Uranium"

end
end--wait
end)--fun

