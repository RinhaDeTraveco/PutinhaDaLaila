if v.Name == "Titanium" then
local New_CFrame = game.Workspace.Minerals.Titanium.CFrame
                    local ts = game:GetService("TweenService")
                    local Char = game.Players.LocalPlayer.Character            
                    local part = Char.HumanoidRootPart
                    local ti = TweenInfo.new(0.5,Enum.EasingStyle.Linear)
                    local tp = {CFrame = New_CFrame}
                    ts:Create(part,ti,tp):Play()
end
end
wait(0.5) timbu.Anchored = true
game.Workspace.Gravity = 0
wait(0.7) timbu.Anchored = false
game.Workspace.Gravity = 196
game.Workspace.Minerals.Titanium.Name = "MinedRock"
end)

